<mapper namespace="com.imooc.o2o.dao.AreaDao">
<select id="queryZone" resultType="com.xlljoy.o2o.entity.Zone">
	SELECT zone_id,name,update_time,create_time,priority
	FROM tb_zone
	ORDER BY priority
	DECS
	</select>
</mapper>